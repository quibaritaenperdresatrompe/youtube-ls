{
  "name": "youtube-ls",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "lint": "eslint --cache .",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": "@qbaepst/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,html,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@qbaepst/prettier-config": "^1.1.0",
    "git-cz": "^3.2.1",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
